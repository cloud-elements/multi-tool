{
  "dependencies": {
    "execa": "0.6.x",
    "findup-sync": "0.4.x",
    "fs-extra": "2.x",
    "path-exists": "3.x",
    "pify": "2.x",
    "ramda": "0.23.x",
    "semver": "5.x",
    "validate-npm-package-name": "2.x",
    "valid-filename": "2.x"
  },
  "description": "Install multiple versions of NPM packages via semver ranges",
  "devDependencies": {
    "ava": "0.17.x",
    "npm-check": "5.x",
    "nyc": "10.x",
    "xo": "0.17.x"
  },
  "engines": {"node": ">=7.6.0"},
  "homepage": "https://github.com/cloud-elements/multi-tool",
  "keywords": [
    "install",
    "local",
    "multiple",
    "npm",
    "package",
    "packages",
    "semver",
    "version",
    "versioning"
  ],
  "license": "ISC",
  "main": "index.js",
  "name": "multi-tool",
  "nyc": {
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cloud-elements/multi-tool.git"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output && rm -rf coverage && rm -rf node_modules",
    "cover": "nyc report --reporter=text-lcov",
    "coverage": "nyc report --reporter=lcov && open ./coverage/lcov-report/index.html",
    "depend": "npm-check",
    "lint": "xo",
    "test": "xo && { npm-check || true; } && nyc --check-coverage ava",
    "unit": "ava"
  },
  "version": "0.2.1",
  "xo": {
    "esnext": true,
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "new-cap": 0
    },
    "space": 2
  }
}
