#!/usr/bin/env bash

rm -rf ./.nyc_output

set -e

echo '### DEPEND ####################################################################'
{ script/depend || true; }
echo '### VULN ######################################################################'
{ script/vuln || true; }
echo '### LINT ######################################################################'
script/lint
echo '### SMALL #####################################################################'
script/small
echo '### COVER #####################################################################'
script/cover

set +e
